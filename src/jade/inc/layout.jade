doctype html(lang='es')
	block vars
head
	meta(charset='utf-8')
	meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
	meta(name='viewport', content='width=device-width')
	block wHead
	if !defaultConfig
		link(rel='stylesheet', href='assets/css/style.css')
	else 
		link(rel='stylesheet', href="#{defaultConfig.rootCSS}main.css")
body 
	block wBody
	block wScripts
	if !defaultConfig
		+loadJQuery('1.11.2')
		<!--[if lt IE 8]>
		script(src='assets/js/libs/respond.js')
		script(src='assets/js/libs/selectivizr.js')
		<![endif]-->
		script(src='assets/js/functions.js')
	else
		script(type="text/javascript", src="#{defaultConfig.rootJS}libs/jquery.js")
		script(type="text/javascript", src="#{defaultConfig.rootJS}libs/respond.js")

